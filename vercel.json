{
    "version": 2,
    "builds": [
      {
        "src": "**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/src/index.html"
      },
      {
        "src": "/index.html",
        "dest": "/src/index.html"
      },
      {
        "src": "/news.html",
        "dest": "/src/news.html"
      },
      {
        "src": "/login.html",
        "dest": "/src/login.html"
      },
      {
        "src": "/foto-video.html",
        "dest": "/src/foto-video.html"
      },
      {
        "src": "/sitemap.html",
        "dest": "/src/sitemap.html"
      },
      {
        "src": "/img/(.*)",
        "dest": "/img/$1"
      },
      {
        "src": "/(.*)\\.css",
        "dest": "/src/$1.css"
      },
      {
        "src": "/(.*)\\.js",
        "dest": "/src/$1.js"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }